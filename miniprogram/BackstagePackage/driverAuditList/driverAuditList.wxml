<!--BackstagePackage/driverAuditList/driverAuditList.wxml-->
<!-- 数据列表 -->
<view class="list_box">
	<view class="list_item" wx:for="{{unPublishList}}" wx:key='index' data-id="{{item['_id']}}" bindtap="Navigate">
		<view class="title">序号：{{index+1}}</view>
		<view class="desc">姓名：{{item.name}}</view>
		<view class="desc">电话：{{item.phone}}</view>
		<view class="desc">驾龄：{{item.drivingAge}}</view>
		<view class="desc">驾驶证：{{item.driving}}</view>
		<view class="desc">审核状态：<text class="text_red">待审核</text></view>
    <view class="desc">上传时间：<text class="text_blue">{{item.ApplyTime}}</text></view>
		<view class="desc">所在地址：{{item.city}}</view>
		<view class="desc text_springgreen">说明：点击卡片查看详情并进行审核发布</view>
	</view>
	<view class="list_item" wx:if='{{unPublishList.length==0}}'>
		<view class="desc" style="text-align: center;">暂时没有待审核的内容</view>
	</view>
</view>


<view class="repulse_img">
	<image bindtap="goRepulse" src="../images/repulse.png"></image>
</view>