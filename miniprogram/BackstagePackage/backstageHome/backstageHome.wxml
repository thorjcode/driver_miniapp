<!--BackstagePackage/backstageHome/backstageHome.wxml-->
<!-- 九宫格 -->
<view class="sudoku_root">
	<view class="sudoku_item" data-url="../driverHome/driverHome" bindtap="Navigate">
		<image class="sudoku_item_img" src="../images/driver.png"></image>
		<text class="sudoku_item_title">代驾审核</text>
	</view>
	<view class="sudoku_item" data-url="../driverState/driverState" bindtap="Navigate">
		<image class="sudoku_item_img" src="../images/driverstate.png"></image>
		<text class="sudoku_item_title">代驾状态</text>
	</view>
	<view class="sudoku_item" data-url="../allOrderHome/allOrderHome" bindtap="Navigate">
		<image class="sudoku_item_img" src="../images/order.png"></image>
