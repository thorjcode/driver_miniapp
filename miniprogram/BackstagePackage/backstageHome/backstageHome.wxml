	</view>
</view>
<view class="sudoku_root">
	<view class="sudoku_item" data-url="../miniAppInfo/miniAppInfo" bindtap="Navigate" wx:if="{{superAdmin}}">
		<image class="sudoku_item_img" src="../images/miniappInfo.png"></image>
		<text class="sudoku_item_title">小程序</text>
	</view>
	<view class="sudoku_item" data-url="../adminList/adminList" bindtap="Navigate" wx:if="{{superAdmin}}">
		<image class="sudoku_item_img" src="../images/admin.png"></image>
